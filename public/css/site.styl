.animated
	-webkit-animation-duration: 0.1s;
	animation-duration: 0.1s;
	-webkit-animation-fill-mode: both;
	animation-fill-mode: both;

.owl-carousel
	h5
		font-size: 0.5em;
		margin-top: 2px;
		letter-spacing: 1px;
		font-weight: 500;
		
	
	h6
		float: right;
		top: -33px;
		font-weight: normal;
		letter-spacing: 2px;
		position: relative

	.item
		.slide
			img
				margin: 0 auto;
				height: 563px;
				width: auto;

.owl-theme
	.owl-controls
		position: fixed;
		bottom 0
		left: 6%;
		z-index 1030

		.owl-nav
			.owl-prev:hover,
			.owl-next:hover
				color green
				background-color transparent
			.owl-prev,
			.owl-next
				color rgb(79, 79, 79);
				background: transparent
				font-size .8em
				border-radius inherit
				letter-spacing 8px
				padding 5px 10px 5px 10px
				/*font-family 'Raleway Thin';*/




.picture
	height 70em


/* dropdown-menu*/
.tp

	.dropdown-menu .sub-menu
		left: 100%;
		position: absolute;
		top: 15px;
		left 277px
		visibility: hidden;



	.dropdown-menu li:hover .sub-menu
		visibility: visible;
		background-color transparent


		li
			background-color #eee
			/*opacity 0.9*/

			a
				margin 0
				padding 5px 10px
				letter-spacing 1px	
				color black
				font-size .6em
				font-weight 700
				font-family 'Helvetica Neue', Helvetica, Arial, sans-serif;
				color rgba(0, 0, 0, 0.8)

			a:hover
				color green

		li:first-child
			padding-top 5px
			
		li:last-child
			padding-bottom 5px

	.dropdown-toggle
		a
			/*font-family 'Raleway Thin';*/
			letter-spacing 8px
			padding 20px 0 10px 0
			width 274px;
			font-weight 700
			/*line-height 1.3em*/
			font-size .8em



	.dropdown > .dropdown-menu
		width 277px

	.dropdown .dropdown-menu	
		opacity 0
		display block

		transition: opacity .2s ease-in-out;
		-moz-transition: opacity .2s ease-in-out;
		-webkit-transition: opacity .2s ease-in-out;

		box-shadow: none;
		border: none;
		font-size: 1em;
		/*background-color black*/

		margin: 0;
		border-radius: 0

		a
			padding 0 0px 2px 0
			margin 0
			color rgb(79, 79, 79);

			/*font-family 'Raleway Thin';*/
			letter-spacing 4px	
			font-size .6em
			font-weight 700

		a:hover
			color green
			background-color #eee	
		a:link
			/*background-color transparent*/


	.dropdown:hover .dropdown-menu
		display: block;
		/*background-color black*/

		opacity: 1.0;
		transition: opacity .6s ease-in-out;
		-moz-transition: opacity .6s ease-in-out;
		-webkit-transition: opacity .6s ease-in-out;

			

