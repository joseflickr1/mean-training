body
	padding-bottom 61px

.navbar-inverse
	background-color rgb(61, 91, 153)
	color rgb(221, 237, 239)
	border none

	.navbar-nav > li > a
		color #fff
		b 
			margin-left 10px
	
	.navbar-nav >.open > a:hover, 
	.navbar-nav >.open > a:focus,
	.navbar-nav >.open > a
		background-color transparent 



.menu-bar
	margin-top 15px
	margin-right 15px
	cursor pointer
	width 20px

	.icon-bar
		background-color #fff
		width 20px
		height 2px
		margin-top 4px
		display block


.navbar-header a.navbar-brand
	margin-top -15px
	margin-bottom -20px
	h6
		color #ccc

footer
	padding-left 15px
	padding-right 15px
	p
		border-top 1px solid #d7e0e2

.navbar-form
	.form-group
		margin-left 10px
	button
		margin-left 10px

.padding-top-med
	padding-top 10px

.margin-left-med
	margin-left 10px

.toast-top-right
	top 50%
	right 50px

.toast-success
	background-color #4C6EB3

#app
	position: fixed
	top: 0
	right: 0
	bottom: 0
	left: 0

.design-play #video-design-play
	width: 100%

.design-play .video-wrapper
	position: relative


.design-play .video-wrapper .video-text
	position: relative
	bottom: 140px
	width: 100%
	height: 0


.design-play .video-wrapper .video-text p
	font-family: 'Roboto', serif
	font-size: 40px
	color: #fff
	margin: auto 50px

.sell-content
	position: relative
	right: 10px

.sell-content .block
	padding: 15px
	
	margin: 50px 0 0 0

.sell-content .block p
	font-family: 'Roboto', serif
	font-size: 27px

.sell-content .block button.btn.btn-primary
	font-size: 14px
	margin-left: 20px
	background-color: rgb(70, 119, 174)
	border: none
