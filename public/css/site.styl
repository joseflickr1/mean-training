/* tp design */


.tp
	font-family: 'Raleway Thin';
	font-size 2em

.navbar-inverse.tp
	background-color transparent

	.navbar-nav > li > a
		color #fff
		background black
		padding 15px 10px 10px 10px
		letter-spacing 6px


.animated
	-webkit-animation-duration: 0.1s;
	animation-duration: 0.1s;
	-webkit-animation-fill-mode: both;
	animation-fill-mode: both;


.owl-theme
	.owl-controls
		position: fixed;
		bottom 0
		left: 6%;
		z-index 1030

